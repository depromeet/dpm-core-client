{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"esModuleInterop": true,
		"incremental": false,
		"isolatedModules": true,
		"lib": ["dom", "dom.iterable", "esnext"],
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"noUncheckedIndexedAccess": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ES2017",
		"allowJs": true
	}
}
